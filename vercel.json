{
  "crons": [
    {
      "path": "/api/cron/sync-deals",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/sync-designer-mockups",
      "schedule": "0 8 * * *"
    }
  ],
  "functions": {
    "app/api/test/robust-deals-sync/route.ts": {
      "maxDuration": 300
    },
    "app/api/test/robust-deals-sync-parallel/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/sync-deals/route.ts": {
      "maxDuration": 300
    },
    "app/api/deals-cache/route.ts": {
      "maxDuration": 30
    },
    "app/api/google-sheets/read/route.ts": {
      "maxDuration": 30
    },
    "app/api/designer-mockups-cache/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/sync-designer-mockups/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/health",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/deals-cache",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/designer-mockups-cache",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/google-sheets/read",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/storage/v1/object/public/avatars/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
